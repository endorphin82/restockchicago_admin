{
  "name": "admin",
  "version": "0.1.0",
  "private": true,
  "homepage": ".",
  "dependencies": {
    "@ant-design/icons": "^4.0.3",
    "@apollo/client": "^3.0.0-beta.43",
    "@apollo/react-hooks": "^3.1.4",
    "@graphql-codegen/cli": "^1.13.2",
    "@graphql-codegen/near-operation-file-preset": "^1.13.2",
    "@graphql-codegen/typescript": "^1.13.2",
    "@graphql-codegen/typescript-operations": "^1.13.2",
    "@graphql-codegen/typescript-react-apollo": "^1.13.2",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/antd": "^1.0.0",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.11.0",
    "@types/react": "^16.9.32",
    "@types/react-dom": "^16.9.6",
    "@types/react-redux": "^7.1.7",
    "antd": "^4.0.4",
    "apollo": "^2.26.0",
    "apollo-boost": "^0.4.7",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-cache-updater": "^0.1.3",
    "apollo-link": "latest",
    "apollo-link-http": "latest",
    "customize-cra": "^0.9.1",
    "graphql": "^14.6.0",
    "react": "^16.13.1",
    "react-apollo": "^3.1.3",
    "react-app-rewired": "^2.1.5",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-scripts": "3.4.1",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "typescript": "^3.8.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "start-antd": "react-app-rewired start",
    "build:react": "react-app-env --env-file=.env.production build",
    "build": "react-app-env --env-file=.env.production build",
    "generate": "graphql-codegen",
    "watch-generate": "graphql-codegen --watch",
    "clear": "find ./src -name \"__generated__\" -exec rm -rf '{}' +",
    "deploy-gh-pages": "npm run build && gh-pages -d build",
    "schema": "apollo client:download-schema --endpoint=http://localhost:3005/graphql ./src/schema.json",
    "types": "apollo client:codegen ./src/__generated__apollo__/types.ts --outputFlat --includes=./src/**/query.ts --addTypename --localSchemaFile=./src/schema.json --target=typescript"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^2.2.0",
    "node-sass": "^4.13.1",
    "react-app-env": "^1.2.3"
  }
}
